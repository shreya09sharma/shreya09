#include <stdio.h>

int main() {
    // Write C code here
    int num;
    printf("enter a num");
    scanf("%d",&num);
    if(num%5==0&&num%11==0)
    {
        printf("the no.is divisible by 5 and 11");
        
    }
    else
    {
        printf("the no.is not divisible by 5 and 11");
    }

    return 0;
}

